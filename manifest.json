{
    "manifest_version": 3,
    "name": "OSINT",
    "version": "1.0",
    "description": "Run OSINT functions and Optimize Searches",
    "permissions": [
        "contextMenus", "storage", "scripting"
    ], 
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "iconSS.png",
        "48": "iconS.png",
        "128": "icon.png"
    }
}