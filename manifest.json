{
    "manifest_version": 3,
    "name": "OSINT",
    "version": "1.0",
    "description": "Run OSINT functions and Optimize Searches",
    "permissions": [
        "contextMenus", "storage", "tabs","scripting"
    ], 
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
    

}